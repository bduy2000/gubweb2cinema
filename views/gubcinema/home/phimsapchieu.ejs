<!--CONTENT-PAGE-->
<div class="content-page">

    <!--MOVIE-TAB-->

    <div class="tab-movie">
        <div class="" data-open="coming">
            <div class="col-inr">
                <% comingsoons.forEach(function(comingsoon) { %>
                
                <div class="film-item cl-ppl t-2d">
                    <a href="/GUB/movies/<%- comingsoon.id %>">
                        <div class="film-item-pic">
                            <img src="/GUB/movie/picture/<%- comingsoon.id %>" alt="THIÊN THẦN HỘ MỆNH (C18)" />
                        </div>
                        <div class="film-item-txt">
                            <h3><%- comingsoon.Name %></h3>
                            <p>
                                <%- comingsoon.Decription %>
                                
                            </p>
                            <p>Khởi chiếu: Từ <%- comingsoon.ReleaseDate %></p>
                        </div>
                    </a>
                    <div class="film-item-type">
                        <span class="icon-2d"></span>
                        <span class="icon-am3d"></span>
                        <span class="icon-3d"></span>
                    </div>
                    <div class="film-item-but">
                        <a class="cart-btn fontsize13" href="/GUB/datve/<%- comingsoon.id %>/<%-cinemas[0] ? cinemas[0].id : '#' %>/2021-05-24 ">Mua vé</a>
                    </div>
                </div>
              
                <%})%>
            </div>
        </div>
    </div>
    <!--MOVIE-TAB-->
</div>
<!--CONTENT-PAGE-->