<!--CONTENT-PAGE-->
<div class="content-page">
    <!--PROMOTION-LIST-->
    <div class="promotion-list-content">
        <div class="section-title">
            <h2>Các rap</h2>
        </div>
        <div class="promotion-list-wrap">
            <% cinemas.forEach(function(cinema) { %>
            <div class="promotion-block" data-target="4">
                <div class="promotion-block-pic">
                    <a class="zoom skew" href="javascript:void(0)">zoom</a>
                    <img src="/GUB/cinema/picture/<%-cinema.id%>" alt="pic" />
                </div>
                <div class="promotion-block-txt">
                    <h3><a href="/GUB/lichchieuchitiet/<%-cinemas[0] ? cinemas[0].id : '#' %>/2021-05-24"><%- cinema.Name -%></a></h3>
                    <p><span style="font-size:16px;"><span style="font-family:arial,helvetica,sans-serif;">- Địa chỉ: </span></span><span style="font-family: arial, helvetica, sans-serif; font-size: 16px;"><%- cinema.Address %></p>
        
                    <iframe  style="height: 300px; width: 600px;" id="ggmap" src="<%- cinema.Map ? 'https://www.google.com/maps/embed?pb='+cinema.Map  : 'https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d15739726.389855992!2d96.86901966300033!3d15.607315091218387!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x31157a4d736a1e5f%3A0xb03bb0c9e2fe62be!2zVmnhu4d0IE5hbQ!5e0!3m2!1svi!2s!4v1622966155760!5m2!1svi!2s' %>"></iframe> 
                </div>
                
            </div>
            <% }) %>
           
        </div>
        <div class="bg-cover" style="background-image:url(/pictures/2_khuyen%20mai/3_back%20ground/bg-promotion-100x100-100x100-100x100.jpg);"></div>
    </div>
    <!--PROMOTION-LIST-->

    <!--PLAYING-MOVIE-->
   

    <!--PLAYING-MOVIE-->


</div>
<!--CONTENT-PAGE-->