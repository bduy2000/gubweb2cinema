<button style="height: 50px; width: 50px;" type="button" class="btn-close" aria-label="Close" onclick="window.location.href='/GUB/admin'"></button>
    <div class="container-contact100">
		<div class="wrap-contact100">
                 <center>
				<span class="contact100-form-title" style="color: #000022; font-weight: bold;font-size:45px">
					List of Theater
				</span>
                </center>

				<div class="container-fluid">

					<!-- Page Heading -->
					<h1 class="h3 mb-2 text-gray-800">THEATER
						<a href="/theater/add" class="btn btn-success btn-circle pull-left">
							<i>+</i>
						</a>
					</h1>
				
					<!-- DataTales Example -->
					<div class="card shadow mb-4">
						<div class="card-body">
							<div class="table-responsive">
								<table class="table table-bordered table-hover" id="dataTable" width="100%" cellspacing="0">
									<thead>
									<tr style="text-align: center; vertical-align: middle;" >
										<th>ID</th>
										<th>NAME</th>
										<th>CINEMA</th>
										<th>CATEGORY</th>
										<th>WIDTH</th>
										<th>HEIGHT</th>
										<th>DEL</th>
										
									</tr>
									</thead>
									
									<tbody>
										<% theaters.forEach(function(theater) { %>
                      					
									<tr style="text-align: center; vertical-align: middle;">
										<td onclick="window.location.href='/theater/update/<%-theater.id-%>'"><%- theater.id %></td>
										<td onclick="window.location.href='/theater/update/<%-theater.id-%>'"><%- theater.Name %></td>
									  <td onclick="window.location.href='/theater/update/<%-theater.id-%>'"><%- theater.Cinema.Name %></td>
										<td onclick="window.location.href='/theater/update/<%-theater.id-%>'"><%- theater.Category%></td>
										<td onclick="window.location.href='/theater/update/<%-theater.id-%>'"><%- theater.Width %></td>
										<td onclick="window.location.href='/theater/update/<%-theater.id-%>'"><%- theater.Height %></td>

						
										<td>
											
												<button onclick="window.location.href='/theater/delete/<%-theater.id-%> '" type="submit" class="btn btn-danger btn-circle btn-sm">
													<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-trash-fill" viewBox="0 0 16 16">
														<path d="M2.5 1a1 1 0 0 0-1 1v1a1 1 0 0 0 1 1H3v9a2 2 0 0 0 2 2h6a2 2 0 0 0 2-2V4h.5a1 1 0 0 0 1-1V2a1 1 0 0 0-1-1H10a1 1 0 0 0-1-1H7a1 1 0 0 0-1 1H2.5zm3 4a.5.5 0 0 1 .5.5v7a.5.5 0 0 1-1 0v-7a.5.5 0 0 1 .5-.5zM8 5a.5.5 0 0 1 .5.5v7a.5.5 0 0 1-1 0v-7A.5.5 0 0 1 8 5zm3 .5v7a.5.5 0 0 1-1 0v-7a.5.5 0 0 1 1 0z"/>
													  </svg>
												</button>
										
										</td>
									

									</tr>
								<% })%>
									</tbody>
								</table>
							</div>
						</div>
					</div>
				
				</div>
	
				
				</div>
    </div>

    
    

