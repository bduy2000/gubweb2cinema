
<button style="height: 50px; width: 50px;" type="button" class="btn-close" aria-label="Close" onclick="window.location.href='/GUB/admin'"></button>

    <div class="container-contact100">
		<div class="wrap-contact100">
                 <center>
				<span class="contact100-form-title" style="color: #000022; font-weight: bold;font-size:45px">
					List of Cinema
				</span>
                </center>

				<div class="container-fluid">

					<!-- Page Heading -->
					<h1 class="h3 mb-2 text-gray-800">Cinema
						<a href="/cinema/add" class="btn btn-success btn-circle pull-left">
							<i>+</i>
						</a>
					</h1>
				
					<!-- DataTales Example -->
					<div class="card shadow mb-4">
						<div class="card-body">
							<div class="table-responsive">
								<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
									<thead>
									<tr>
										<th>ID</th>
										<th>Name</th>
										<th>Address</th>
										<th>Update</th>
										<th>Delete</th>
									</tr>
									</thead>
									<tfoot>
									<tr>
										<th>ID</th>
										<th>Name</th>
										<th>Address</th>
										<th>Update</th>
										<th>Delete</th>
									</tr>
									</tfoot>
									<tbody>
										<% cinemas.forEach(function(cinema) { %>
									<tr>
										<td><%- cinema.id %></td>
										<td><%- cinema.Name %></td>
										<td><%- cinema.Address %></td>
										<td>
										<button onclick="window.location.href='/cinema/update/<%- cinema.id %>'" class="btn btn-primary btn-circle btn-sm">
										<i class="fas fa-trash"></i>
										</button>
										</td>
										<td>
										<button onclick="window.location.href='/cinema/delete/<%- cinema.id %>'" class="btn btn-danger btn-circle btn-sm">
											<i class="fas fa-trash"></i>
										</button>
										</td>
										

									</tr>
									<% })%>
								
									</tbody>
								</table>
							</div>
						</div>
					</div>
				
				</div>
	
				
				</div>
    </div>

    
    

