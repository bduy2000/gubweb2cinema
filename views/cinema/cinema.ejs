
<button style="height: 50px; width: 50px;" type="button" class="btn-close" aria-label="Close" onclick="window.location.href='/GUB/admin'"></button>

    <div class="container-contact100">
		<div class="wrap-contact100">
                 <center>
				<span class="contact100-form-title" style="color: #000022; font-weight: bold;font-size:45px">
					List of Cinema
				</span>
                </center>

				<div class="container-fluid">

					<!-- Page Heading -->
					<h1 class="h3 mb-2 text-gray-800">Cinema
						<a href="/cinema/add" class="btn btn-success btn-circle pull-left">
							<i>+</i>
						</a>
					</h1>
				
					<!-- DataTales Example -->
					<div class="card shadow mb-4">
						<div class="card-body">
							<div class="table-responsive">
								<table class="table table-bordered table-hover" id="dataTable" width="100%" cellspacing="0">
									<thead>
									<tr style="text-align: center; vertical-align: middle;" >
										<th>ID</th>
										<th>Picture</th>
										<th>Name</th>
										<th>Address</th>
										<th>Map</th>
										<th>Del</th>
									</tr>
									</thead>
									
									<tbody>
										<% cinemas.forEach(function(cinema) { %>
									<tr style="text-align: center; vertical-align: middle;" >
										<td onclick="window.location.href='/cinema/update/<%- cinema.id %>'"><%- cinema.id %></td>
										<td onclick="window.location.href='/cinema/update/<%- cinema.id %>'">
											<img src="/cinema/picture/<%- cinema.id %>" style="width:300px;height:200px;display: block; margin-left: auto; margin-right: auto" id="output"  />
										</td>

										<td onclick="window.location.href='/cinema/update/<%- cinema.id %>'"><%- cinema.Name %></td>
										<td onclick="window.location.href='/cinema/update/<%- cinema.id %>'"><%- cinema.Address %></td>
										<td onclick="window.location.href='/cinema/update/<%- cinema.id %>'">
										<iframe style="height: 200px; width: 300px;" id="ggmap" src="<%- cinema.Map ? 'https://www.google.com/maps/embed?pb='+cinema.Map  : 'https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d15739726.389855992!2d96.86901966300033!3d15.607315091218387!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x31157a4d736a1e5f%3A0xb03bb0c9e2fe62be!2zVmnhu4d0IE5hbQ!5e0!3m2!1svi!2s!4v1622966155760!5m2!1svi!2s' %>"></iframe> 
										</td>
										
										
										<td>
										<button onclick="window.location.href='/cinema/delete/<%- cinema.id %>'" class="btn btn-danger btn-circle btn-sm">
											<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-trash-fill" viewBox="0 0 16 16">
												<path d="M2.5 1a1 1 0 0 0-1 1v1a1 1 0 0 0 1 1H3v9a2 2 0 0 0 2 2h6a2 2 0 0 0 2-2V4h.5a1 1 0 0 0 1-1V2a1 1 0 0 0-1-1H10a1 1 0 0 0-1-1H7a1 1 0 0 0-1 1H2.5zm3 4a.5.5 0 0 1 .5.5v7a.5.5 0 0 1-1 0v-7a.5.5 0 0 1 .5-.5zM8 5a.5.5 0 0 1 .5.5v7a.5.5 0 0 1-1 0v-7A.5.5 0 0 1 8 5zm3 .5v7a.5.5 0 0 1-1 0v-7a.5.5 0 0 1 1 0z"/>
											  </svg>
										</button>
										</td>
										

									</tr>
									<% })%>
								
									</tbody>
								</table>
							</div>
						</div>
					</div>
				
				</div>
	
				
				</div>
    </div>

    
    

